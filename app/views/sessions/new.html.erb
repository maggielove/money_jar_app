<div class="sessions-form">
  <h1>Log In</h1>

  <div class="form">
    <%= form_tag '/login' do %>

    <div id="login-labels">
      <p>
        <%= label_tag :email %>
        <%= text_field_tag :email %>
      </p>
      <p>
        <%= label_tag :password %>
        <%= password_field_tag :password %>
      </p>
    </div>

      <%= submit_tag "Log in" %>

    <% end %>
  </div>
</div>
