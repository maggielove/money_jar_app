<!DOCTYPE html>
<html>
<head>
  <title>MoneyJarApp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div id="session-info">
      <% if current_user %>
        Signed in under <%= current_user.email %> | <%= link_to "Log Out", '/logout' %>
      <% else %>
        <%= link_to "Log In", '/login' %> | <%= link_to "Sign Up", new_user_path %>
      <% end %>
  </div>

    <%= yield %>

</body>
</html>
